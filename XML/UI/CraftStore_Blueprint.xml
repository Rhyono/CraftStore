                <Backdrop name="$(parent)BG" centerColor="101010" edgeColor="202020">
                  <Anchor point="128"/>
                  <Dimensions x="34" y="36"/>
                  <Edge edgeSize="1"/>
                </Backdrop>
                <Texture name="$(parent)Texture" textureFile="esoui/art/inventory/inventory_tabicon_craftbag_blacksmithing_up.dds" color="CCAA1A">
                  <Dimensions y="32" x="32"/>
                  <Anchor point="128"/>
                </Texture>
