              <OnInitialized>self:SetText(GetString(SI_GAMEPAD_HELP_SEARCH)..'...')</OnInitialized>
              <OnFocusGained>self:SetText("")</OnFocusGained>
              <OnFocusLost>CraftStoreFixedAndImprovedLongClassName.BlueprintSearch()</OnFocusLost>
